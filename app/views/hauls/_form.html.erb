

   <%= form_with model: @haul,local: true do |f|%>
    <%= f.label :subject %>
    <%= f.text_field :subject %>
    <br>
     <%= f.label :store %>
    <%= f.text_field :store_name %>
     <br>
     <%= f.label :city %>
    <%= f.text_field :city_name %>
    <br>
    <%= f.label :product_image %>
    <%= f.text_field :product_image %>
    <br>
    <%= f.label :discreption %>
    <%= f.text_field :discreption %>
     <br>
    <%= f.submit "Save"%> <%= link_to 'Back', hauls_path %>
<% end %>
